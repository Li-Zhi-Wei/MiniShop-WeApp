.my-container{
    background-color:#EDEDED;
}

.my-address{
    background-color: #fff;
    width: 100vw;
}
.section{
    border-bottom:1rpx solid #E9E9E9;
    margin-left:30rpx;
    height: 90rpx;
}
.section:last-child{
    border-bottom:none;
}
.section input{
    font-size: 28rpx;
    color:#000;
    padding: 20rpx 30rpx 20rpx 0;
}
.item-title{
    padding: 25rpx 0;
    color: #999999;
    text-align: center;
    border-bottom: 1rpx solid #EDEDED;
    font-size: 28rpx;
    background-color: #fff;
    position: relative;
}
.add-new-address{
    width: 100%;
    height: 90rpx;
    font-size:28rpx;
    color:#989898;
    display: flex;
    align-items: center;
    justify-content: center;
}
.add-new-address .icon{
    color:#AB956D;
    margin-right: 10rpx;
}